#include <bits/stdc++.h>
using namespace std;

void solve(){
    int i, j, n, m;
    cin>>n;
    int a[n],b[n],c[n],d[n];
    for(i=0;i<n;i++){
        cin>>a[i];
    }
    for(i=0;i<n;i++){
        cin>>b[i];
    }
    for(i=0;i<n;i++){
        cin>>c[i];
    }
    for(i=0;i<n;i++){
        if(i==0){d[i]=a[i];m=d[i];}
        else {
            if(a[i]!=m){
                d[i]=a[i];
                m=a[i];
            }
            else {
                d[i]=c[i];
                m=c[i];
            }
        }
    }
    if(d[0]==d[n-1]){
        if(d[0]!=a[n-1] && d[n-2]!=a[n-1]){
            d[n-1]=a[n-1];
        }
        else if(d[0]!=b[n-1] && d[n-2]!=b[n-1]){
            d[n-1]=b[n-1];
        }
        else {
            d[n-1]=c[n-1];
        }
    }
    for(i=0;i<n;i++){
        cout<<d[i]<<" ";
    }
    cout<<'\n';
}

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    solve();
	}
	return 0;
}
